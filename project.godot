config_version=5

[application]

config/name="Proto RPG v0.000296"
config/tags=PackedStringArray("rpg")
run/main_scene="res://data/scenes/main/main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

BattleManager="*res://data/scripts/autoloads/battle_manager.gd"
SceneStacker="*res://data/scripts/autoloads/scene_stacker.gd"
GameManager="*res://data/scripts/autoloads/game_manager.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://data/items/": "orange",
"res://data/scenes/": "gray",
"res://data/scripts/": "blue",
"res://data/sprites/": "pink"
}

[global_group]

active_player_party="A party member that is currently active in battle."
nonactive_party_member="A party member that is currently not active in battle."

